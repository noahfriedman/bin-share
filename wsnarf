#! /bin/sh

# $Id: wsnarf,v 1.7 2005/06/01 13:07:38 friedman Exp $

exec wget            \
  --convert-links    \
  --backup-converted \
  --force-html       \
  --no-parent        \
  --recursive        \
  --level=0          \
  --page-requisites  \
  --reject 'index.*\??=*' \
  ${1+"$@"}

# wsnarf ends here
