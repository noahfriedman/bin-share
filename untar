#! /bin/sh
# $Id: untar,v 1.3 1997/09/23 19:27:43 friedman Exp $

for f in ${1+"$@"}; do
  case "$f" in
    *.t[ag]z | *.tar.gz | *.tar.[Zz] | *.nif )
      ${GZIP-gzip} -dc "$f" | ${TAR-tar} -xpvf -
     ;;
    *.tbz | *.tbz2 | *.tar.bz2 )
      ${BZIP2-bzip2} -dc "$f" | ${TAR-tar} -xpvf -
     ;;
    *.tar )
      ${TAR-tar} -xpvf "$f"
     ;;
    *.zip | *.jar )
      ${UNZIP-unzip} -oL "$f"
     ;;
  esac
done

# eof
